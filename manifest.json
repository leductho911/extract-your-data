{
    "manifest_version": 3,
    "name": "Extract Custom Data",
    "author": {
        "email": "leductho911@gmail.com"
    },
    "version": "1.0.0",
    "description": "Extract and copy data matching the format Cxxxx, Cxxx or Cxx from the current webpage.",
    "permissions": ["activeTab", "scripting", "storage"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    }
  }