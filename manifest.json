{
    "manifest_version": 3,
    "name": "Extract Your Data",
    "author": {
        "email": "leductho911@gmail.com"
    },
    "version": "1.0.1",
    "description": "A chrome extension that extract custom data on your opening tab",
    "permissions": ["activeTab", "scripting", "storage"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "options_ui": {
        "page": "settings.html",
        "open_in_tab": true
    }
  }